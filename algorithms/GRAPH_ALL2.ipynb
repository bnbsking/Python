    "G = matrix2Graph(M)\n",
    "def dfs(G):\n",
    "    ans = []\n",
    "    def search(node, visit):\n",
    "        nonlocal ans\n",
    "        if node:\n",
    "            ans.append( node.val )\n",
    "            visit.add( id(node) )\n",
    "            for neighbor in filter(lambda N:id(N) not in visit, node.neighbors):\n",
    "                search( neighbor, visit )\n",
    "    search(G, set())\n",
    "    return ans\n",
    "\n",
    "print( dfs(G) )"
