    "G = matrix2Graph(M)\n",
    "def graph2Matrix(G,n):\n",
    "    M = [ [0]*n for i in range(n) ]\n",
    "    def dfs(node,visit):\n",
    "        nonlocal M\n",
    "        if node:\n",
    "            visit.add( id(node) )\n",
    "            for neighbor in filter(lambda N:id(N) not in visit, node.neighbors):\n",
    "                M[node.val][neighbor.val], M[neighbor.val][node.val] = 1, 1\n",
    "                dfs(neighbor, visit)\n",
    "    dfs(G,set())\n",
    "    return M\n",
    "\n",
    "for row in graph2Matrix(G,4):\n",
    "    print(row)"
   ]
